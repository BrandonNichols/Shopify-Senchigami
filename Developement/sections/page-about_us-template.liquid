{% schema %}
  {
    "name": "Image Gallery",
    "blocks": [
      {  
        "type": "image",
        "name": "images",
        "settings": [
          {
            "type": "image_picker",
            "id": "Image",
            "label": "Image that will be used in the slideshow"
          }
        ]
      }
    ]
  }
{% endschema %}
